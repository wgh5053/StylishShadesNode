        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="https://media.sunglasshut.com/cms/resource/image/458130/landscape_ratio1440x465/1920/620/1e27dfadd6edba15ae7817341ff97c66/AAEDE4685B052E7986EC1D531B5686E5/hp-z1-1bis-desktop.jpg" alt="Los Angeles" style="width:100%;">
            </div>

            <div class="item">
                <img src="https://thumbs.dreamstime.com/b/children-eat-watermelon-beach-sunglasses-banner-long-format-children-eat-watermelon-beach-sunglasses-banner-152370218.jpg" alt="Chicago" style="width:100%;">
            </div>

            <div class="item">
                <img src="https://thumbs.dreamstime.com/b/beautiful-thoughtful-mixed-race-african-american-female-girl-child-teenager-young-woman-sunshine-wearing-blue-reflective-136253832.jpg" alt="New york" style="width:100%;">
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
